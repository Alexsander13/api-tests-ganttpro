  "endpoints": [
    {
      "method": "PUT",
      "path": "/tasks/{taskId}",
      "operationId": "updateTask",
      "summary": "Update task",
      "pathParams": {
        "taskId": { "type": "integer", "required": true }
      },
      "body": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "description": { "type": "string" },
          "status": { "type": "integer", "enum": [1, 2, 3, 4], "enumLabels": ["Open", "In progress", "Done", "Closed"] },
          "startDate": { "type": "string" },
          "endDate": { "type": "string" },
          "duration": { "type": "integer" },
          "priority": { "type": "integer", "enum": [1, 2, 3, 4, 5], "enumLabels": ["Lowest", "Low", "Medium", "High", "Highest"] },
          "progress": { "type": "number", "min": 0, "max": 1 },
          "parent": { "type": "integer" },
          "estimation": { "type": "integer" },
          "deadline": { "type": "string" },
          "color": { "type": "integer", "min": 1, "max": 18 },
          "customFields": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "customFieldId": { "type": "integer" },
                "value": { "type": "any" }
              }
            }
          }
        }
      },
      "notes": {
        "dateRecalculationRules": [
          "Only startDate => endDate auto recalculated",
          "Only duration => endDate auto recalculated",
          "Only endDate => duration auto recalculated",
          "startDate+endDate (no duration) => duration auto recalculated",
          "startDate+duration (no endDate) => endDate auto recalculated",
          "endDate+duration (no startDate) => endDate auto recalculated",
          "startDate+endDate+duration => endDate ignored; recalculated from duration"
        ],
        "projectTypeTaskExceptions": [
          "Type",
          "Progress",
          "Status",
          "Priority",
          "Duration",
          "Estimation",
          "End Date",
          "Resources"
        ]
      },
      "responses": {
        "200": { "example": { "status": "ok" } },
        "400": {},
        "401": {}
      }
    },
